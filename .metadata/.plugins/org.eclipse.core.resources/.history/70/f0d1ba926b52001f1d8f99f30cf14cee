package com.blogs.dto;

import java.time.LocalDateTime;

public class ApiResponce {
	private String message;
	private LocalDateTime timesStamp;

	public ApiResponce() {
		
	}

	public ApiResponce(String message) {
		super();
		this.message = message;
		this.timesStamp =LocalDateTime.now();
	}

	public String getMessage() {
		return message;
	}

	public void setMessage(String message) {
		this.message = message;
	}

	public LocalDateTime getTimesStamp() {
		return timesStamp;
	}

	public void setTimesStamp(LocalDateTime timesStamp) {
		this.timesStamp = timesStamp;
	}

	@Override
	public String toString() {
		return "ApiResponce [message=" + message + ", timesStamp=" + timesStamp + "]";
	}
	
	
}
